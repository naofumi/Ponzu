- cache ['booth_session_v2', @sessions.max_by{|s| s.updated_at}, @show_date] do
  = ponzu_frame :id => ["booth", params[:id]],
                :class => "backdrop",
                :title_args => {:day => l(@show_date, :format => :month_day)} do
    .fixed_sub_header
      %div
        .radio_button_set.button-group
          = link_to t('booth_sessions.sub_menu.show_as_map'), ksp(booth_session_path(@show_date.strftime('%Y-%m-%d'))), 
                    :class => ['ks_tab', "booth_#{@day}_menu_map",  "button", "primary"]
          = link_to t('booth_sessions.sub_menu.show_as_list'), ksp(list_booth_session_path(@show_date.strftime('%Y-%m-%d'))), 
                    :class => ['ks_tab', "booth_#{@show_date.strftime('%Y-%m-%d')}_menu_list",  "button"]

    = render :partial => "booth_hall"
